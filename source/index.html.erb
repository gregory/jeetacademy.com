---
title: Jeet Academy
homepage: true
---

<% content_for :header do %>
  <div class="clear"></div>
  <ul id="highlight-box">
    <li class="first">
      <% link_to '/courses/disciplines/sanda/' do %>
        <span class="hide molot">Sanda-MMA</span>
      <% end %>
    </li>
    <li>
      <% link_to '/courses/disciplines/jeet_kune_do/' do %>
        <span class="hide molot">Jeet Kune Do</span>
      <% end %>
    </li>
    <li class="last">
      <% link_to '/courses/disciplines/silat/' do %>
        <span class="hide molot">Filipino Martial Arts</span>
      <% end %>
    </li>
    <span class="pitch-bg"></span>
    <span class="pitch-shadow"></span>
  </ul>
<% end %>

<div class="main-content homepage">

  <div id="myCarousel" class="carousel slide">
    <div class="carousel-inner">

        <div class="item">
          <%= image_tag "bruce_lee.52139.jpg" %>
        </div>

        <div class="item">
          <%= image_tag "Homepage_3.jpg" %>
        </div>

        <div class="item active">
          <%= image_tag "Hompage_2B.jpg" %>
        </div>

    </div>
    <div class="mask-carousel"></div>
    <a class="carousel-control btn left molot" href="#myCarousel" data-slide="prev">‹</a>
    <a class="carousel-control btn right molot" href="#myCarousel" data-slide="next">›</a>
  </div>
</div>

